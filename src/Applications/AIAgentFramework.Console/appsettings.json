{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LLM": {
    "DefaultProvider": "local",
    "OpenAI": {
      "ApiKey": "YOUR_OPENAI_API_KEY_HERE",
      "BaseUrl": "https://api.openai.com/v1",
      "DefaultModel": "gpt-4-turbo"
    },
    "Claude": {
      "ApiKey": "YOUR_CLAUDE_API_KEY_HERE",
      "BaseUrl": "https://api.anthropic.com/v1",
      "DefaultModel": "claude-3-5-sonnet-20241022"
    },
    "Local": {
      "BaseUrl": "http://192.168.25.50:11434",
      "DefaultModel": "gpt-oss:20b"
    },
    "Roles": {
      "Assistant": {
        "Provider": "openai",
        "Model": "gpt-4-turbo"
      },
      "Analyzer": {
        "Provider": "claude",
        "Model": "claude-3-5-sonnet-20241022"
      },
      "Coder": {
        "Provider": "openai",
        "Model": "gpt-4-turbo"
      }
    }
  },
  "Tools": {
    "WebSearch": {
      "ApiKey": "YOUR_SEARCH_API_KEY_HERE",
      "BaseUrl": "https://api.search.example.com",
      "MaxResults": 10,
      "RequestsPerMinute": 60
    },
    "Database": {
      "ConnectionString": "Server=localhost;Database=AIAgentDB;Trusted_Connection=true;",
      "Provider": "SqlServer"
    }
  },
  "State": {
    "Provider": "InMemory",
    "Redis": {
      "ConnectionString": "localhost:6379",
      "Database": 0,
      "KeyPrefix": "aiagent:"
    }
  },
  "Orchestration": {
    "MaxConcurrentTasks": 5,
    "DefaultTimeout": "00:05:00",
    "RetryPolicy": {
      "MaxRetries": 3,
      "BackoffMultiplier": 2.0,
      "InitialInterval": "00:00:01"
    }
  },
  "Telemetry": {
    "Enabled": true,
    "ExporterType": "Console",
    "ServiceName": "AIAgentFramework.Console",
    "ServiceVersion": "1.0.0"
  }
}