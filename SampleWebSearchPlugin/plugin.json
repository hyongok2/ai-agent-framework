{
  "id": "sample-web-search-plugin",
  "name": "Sample Web Search Plugin",
  "description": "웹에서 정보를 검색하는 샘플 플러그인 도구",
  "version": "1.0.0",
  "author": "AI Agent Framework Team",
  "authorEmail": "team@aiagentframework.com",
  "homepage": "https://github.com/aiagentframework/sample-plugins",
  "license": "MIT",
  "assemblyPath": "SampleWebSearchPlugin.dll",
  "mainClass": "SampleWebSearchPlugin.WebSearchTool",
  "dependencies": [
    {
      "name": "System.Net.Http",
      "version": "8.0.0",
      "type": "NuGet",
      "required": true,
      "description": "HTTP 클라이언트 라이브러리"
    },
    {
      "name": "System.Text.Json",
      "version": "8.0.0",
      "type": "NuGet",
      "required": true,
      "description": "JSON 직렬화 라이브러리"
    }
  ],
  "configurationSchema": {
    "type": "object",
    "properties": {
      "search_api_url": {
        "type": "string",
        "description": "검색 API URL",
        "default": "https://api.example.com/search"
      },
      "api_key": {
        "type": "string",
        "description": "API 키 (선택사항)",
        "default": ""
      },
      "timeout_seconds": {
        "type": "integer",
        "description": "요청 타임아웃 (초)",
        "default": 30
      },
      "max_retries": {
        "type": "integer",
        "description": "최대 재시도 횟수",
        "default": 3
      }
    }
  },
  "defaultConfiguration": {
    "search_api_url": "https://api.example.com/search",
    "api_key": "",
    "timeout_seconds": 30,
    "max_retries": 3
  },
  "tags": [
    "web",
    "search",
    "internet",
    "information",
    "sample"
  ],
  "minFrameworkVersion": "1.0.0",
  "maxFrameworkVersion": "2.0.0",
  "createdAt": "2024-01-01T00:00:00Z",
  "updatedAt": "2024-01-01T00:00:00Z"
}